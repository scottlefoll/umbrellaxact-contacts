{
  "swagger": "2.0",
  "info": {
    "title": "MongoDB Contact API",
    "description": "CSE 331 W04 MongoDB Contact API",
    "version": "1.0.0"
  },
  "host": "https://umbrellaxact-contacts.onrender.com/",
  "basePath": "",
  "schemes": [
    "https"
  ],
  "paths": {
    "/api-docs": {
      "get": {
        "description": "The API documentation for MongoDB Contacts API",
        "parameters": [],
        "responses": {}
      }
    },
    "/contact": {
      "get": {
        "description": "Get all contacts in MongoDB Contact API",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/contact/{id}": {
      "get": {
        "description": "Get single contact by id (ie. 'avatar_2009'), case insensitive",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/create": {
      "post": {
        "description": "Create a contact",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "_id": {
                  "example": "sarah_kim"
                },
                "FName": {
                    "example": "Sarah"
                  },
                "LName": {
                  "example": "Kim"
                },
                "Address": {
                    "example": "2907 Shelburne Rd"
                  },
                "City": {
                  "example": "Burlington"
                },
                "State": {
                    "example": "VT"
                  },
                "Zip": {
                  "example": "05909"
                },
                "Country": {
                    "example": "USA"
                  },
                "Email": {
                  "example": "saraKim@gmail.com"
                },
                "Phone": {
                    "example": "802-575-3950"
                  },
                "Website": {
                  "example": "ncrealtors.com"
                },
                "Company": {
                    "example": "North Country Realtors"
                  },
                "JobTitle": {
                  "example": "Broker"
                },
                "ContactType": {
                    "example": "Business"
                },
                "ContactMethod": {
                  "example": "Phone"
                },
                "Notes": {
                    "example": "Runs a successful real estate agency."
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          }
        }
      }
    }
  }
}